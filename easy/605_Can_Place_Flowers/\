#include <stdio.h>

bool canPlaceFlowers(int* flowerbed, int flowerbedSize, int n) {
 for (int i = 0; i < flowerbedSize; i++) {
  if ()


  if (i != 0 && i != flowerbedSize-1) {
   if ((flowerbed[i-1] | flowerbed[i+1]) == 0) {
    flowerbed[i]++;
    n--;
   }
  } else if (i == 0) {
   if (flowerbed[i+1] == 0) {
    flowerbed[i]++;
    n--;
   }
  } else {
   if (flowerbed[i-1] == 0) {
    flowerbed[i]++;
    n--;
   }
  }



 }

 return n == 0;
}

int main (void) {

 return 0;
}
